{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "plugins": ["jsdoc"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-var": 0,
    "require-jsdoc": 0,
    "max-len": 0,
    "camelcase": 0,
    "prefer-const": 0,
    "valid-jsdoc": 0,
    "no-throw-literal": 0,
    "no-unused-vars": 0,
    "quotes": [0, "single", "double"],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "never"
      }
    ],
    "linebreak-style": 0
  }
}
