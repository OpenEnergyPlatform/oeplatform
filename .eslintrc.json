{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "plugins": ["jsdoc"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": 0,
    "prefer-const": 0,
    "valid-jsdoc": [
      2,
      {
        "prefer": { "return": "returns" },
        "requireReturn": false,
        "requireReturnDescription": false,
        "requireParamDescription": false
      }
    ],
    "quotes": [0, "single", "double"],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "never"
      }
    ],
    "linebreak-style": 0
  }
}
