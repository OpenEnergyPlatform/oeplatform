  "scripts": {
    "start": "react-scripts start",
    "create": "rm -rf ../static && npm run build",
    "build": "BUILD_PATH='../static/temp' react-scripts build && npm run build:make_dir",
    "build:make_dir": "mkdir -p ../static/factsheet/css && mkdir -p ../static/factsheet/js && npm run build:relocate",
    "build:relocate": "mv ../static/temp/static/css ../static/factsheet/ && mv ../static/temp/static/js ../static/factsheet/ && npm run build:clean",
    "build:clean": "rm -rf ../static/temp && cd ../static/factsheet/js && mv main*.js main.js && cd ../css && mv main*.css main.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
