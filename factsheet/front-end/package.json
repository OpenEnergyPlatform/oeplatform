{
  "name": "OEP_Factsheets",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.5.0",
    "@react-hook/mouse-position": "^4.1.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "apollo-boost": "^0.4.9",
    "axios": "^1.1.3",
    "graphql": "^15.8.0",
    "jquery": "^3.6.0",
    "jsonld": "^8.1.0",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.2",
    "react-force-graph": "^1.41.12",
    "react-force-graph-2d": "^1.23.10",
    "react-force-graph-3d": "^1.21.12",
    "react-force-graph-ar": "^1.5.7",
    "react-force-graph-vr": "^1.14.8",
    "react-scripts": "^5.0.0",
    "three-spritetext": "^1.6.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "create": "rm -rf ../static && npm run build",
    "build": "BUILD_PATH='../static/temp' react-scripts build && npm run build:make_dir",
    "build:make_dir": "mkdir -p ../static/factsheet/css && mkdir -p ../static/factsheet/js && npm run build:relocate",
    "build:relocate": "mv ../static/temp/static/css ../static/factsheet/ && mv ../static/temp/static/js ../static/factsheet/ && npm run build:rename",
    "build:rename": "mv ../static/factsheet/js/main*.js ../static/factsheet/js/main.js && mv ../static/factsheet/css/*.css ../static/factsheet/css/main.css && npm run build:clean",
    "build:clean": "rm -rf ../static/temp",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
